{
  "name": "signalflow",
  "version": "1.0.0",
  "description": "SignalFlow - Product Feedback Intelligence for PMs",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create signalflow-db",
    "db:setup": "wrangler d1 execute signalflow-db --file=schema.sql",
    "db:seed": "wrangler d1 execute signalflow-db --file=seed.sql"
  },
  "devDependencies": {
    "wrangler": "^4.59.3"
  }
}
